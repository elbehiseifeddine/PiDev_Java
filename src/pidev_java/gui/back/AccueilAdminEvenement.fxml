            <TableColumn fx:id="actionsEvent" prefWidth="75.0" text="Actions" />
        </columns>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="30.0" />
         </VBox.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
